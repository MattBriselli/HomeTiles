{
    "manifest_version": 2,
    "name": "Custom Tiles",
    "version": "0.1",
    "browser_action": {
        "default_icon": "home-tiles.jpg"
    },
    "chrome_url_overrides" : {
        "newtab": "index.html"
    },
    "permissions": [
      "storage", "geolocation"
    ],
    "content_security_policy": "script-src 'self' https://apis.google.com/js/api.js; object-src 'self'"
}